{
    "name": "invitation-bot-manager",
    "version": "1.0.0",
    "description": "A discordjs bot that manages invitations count for a contest",
    "main": "./src/index.ts",
    "scripts": {
        "start": "NODE_ENV=production node ./build/index.js",
        "start:windows": "SET NODE_ENV=production&& node ./build/index.js",
        "dev": "NODE_ENV=development ts-node ./src/index.ts",
        "dev:windows": "SET NODE_ENV=development&& ts-node ./src/index.ts",
        "dev-watch": "ts-node-dev ./src/index.ts",
        "build": "tsc"
    },
    "keywords": [],
    "readmeFilename": "README.md",
    "license": "GPL-3.0 License",
    "dependencies": {
        "@discordjs/opus": "^0.6.0",
        "discord.js": "^13.0.1",
        "dotenv": "^10.0.0",
        "lodash": "^4.17.21",
        "sqlite3": "^5.0.2"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.173",
        "@types/node": "^16.4.10",
        "@types/sqlite3": "^3.1.7",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "prettier": "^2.2.1",
        "ts-node": "^10.1.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5"
    }
}
